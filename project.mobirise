          "_name": "footer01",
          "_customHTML": "<section group=\"Footers\" global=\"\" once=\"footers\" mbr-class=\"{'mbr-reveal': reveal, 'mbr-parallax-background': bg.parallax}\">\n\n    <mbr-parameters>\n        <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <input type=\"checkbox\" title=\"Full Screen\" name=\"fullScreen\">\n        <input type=\"range\" inline=\"\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"8\" step=\"1\" value=\"4\" condition=\"fullScreen == false\">\n        <input type=\"range\" inline=\"\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"8\" step=\"1\" value=\"6\" condition=\"fullScreen == false\">\n        <input type=\"checkbox\" title=\"Show Logo\" name=\"showLogo\" checked=\"\">\n        <input type=\"checkbox\" title=\"Show Item Title\" name=\"showcardTitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Show Item Text\" name=\"showText\" checked=\"\">\n        <input type=\"checkbox\" title=\"Show Phone\" name=\"showPhone\" checked=\"\">\n        <input type=\"color\" title=\"Border Color\" name=\"border\" value=\"#808080\" selected=\"\">\n        <input type=\"range\" inline=\"\" title=\"Col\" name=\"menuItems1\" min=\"1\" max=\"4\" step=\"1\" value=\"4\">\n        <fieldset type=\"background\" name=\"bg\" parallax=\"\">\n            <input type=\"image\" title=\"Background Image\" value=\"../_images/background1.jpg\" parallax=\"\">\n            <input type=\"color\" title=\"Background Color\" value=\"#232323\" selected=\"\">\n            <input type=\"video\" title=\"Background Video\" value=\"https://www.youtube.com/watch?v=36YgDDJ7XSc\">\n        </fieldset>\n\n        <input type=\"checkbox\" title=\"Gradient\" name=\"gradientBg\" condition=\"bg.type == 'color'\">\n        <input type=\"color\" title=\"Color 2\" name=\"color2\" value=\"#000000\" condition=\"gradientBg &amp;&amp; bg.type == 'color'\">\n\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked=\"\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#0e0e0e\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n        <input type=\"range\" inline=\"\" title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.8\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n        <!-- End block parameters -->\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay &amp;&amp; bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n\n    <div class=\"container\">\n        <div class=\"row mbr-white justify-content-center\">\n\n\n            <div class=\"col-md-12 col-lg-12 pb-5 my-auto md-pb\" mbr-if=\"showLogo\">\n                <div class=\"logo-wrap\">\n                    <img class=\"logo\" src=\"../_images/logo2.png\" alt=\"Mobirise\">\n                </div>\n            </div>\n\n\n\n\n\n\n            <div class=\"col-md-6 align-center col-lg-3\" mbr-if=\"menuItems1 > 0\">\n                <div class=\"item-content\">\n                    <h3 class=\"mbr-section-title pb-2 mbr-bold mbr-fonts-style\" mbr-if=\"showcardTitle\" mbr-theme-style=\"display-7\">Address</h3>\n                    <p class=\"mbr-text pb-3 mbr-fonts-style\" mbr-if=\"showText\" data-app-selector=\".mbr-text, .mbr-section-btn\" mbr-theme-style=\"display-4\">570 8th Ave,\n                        <br>New York, NY 10018\n                        <br>United States</p>\n                </div>\n\n\n            </div>\n\n            <div class=\"col-md-6 col-lg-3 align-center\" mbr-if=\"menuItems1 > 1\">\n                <div class=\"item-content\">\n                    <h3 class=\"mbr-section-title pb-2 mbr-bold mbr-fonts-style\" mbr-if=\"showcardTitle\" mbr-theme-style=\"display-7\">Book a table</h3>\n                    <p class=\"mbr-text mbr-fonts-style\" mbr-if=\"showText\" data-app-selector=\".mbr-text, .mbr-section-btn\" mbr-theme-style=\"display-4\">Dogfood och Sliders\n                        foodtruck.\n                        <br>Under Om oss kan ni läsa\n                    </p>\n                    <h4 class=\"phone mbr-bold pb-5 mbr-fonts-style\" mbr-if=\"showPhone\" mbr-theme-style=\"display-7\">(850)\n                        435-4155</h4>\n\n                </div>\n\n            </div>\n\n\n\n            <div class=\"col-md-6 col-lg-3 align-center\" mbr-if=\"menuItems1 > 2\">\n                <div class=\"item-content\">\n                    <h3 class=\"mbr-section-title pb-2 mbr-bold mbr-fonts-style\" mbr-if=\"showcardTitle\" mbr-theme-style=\"display-7\">Opening hours&nbsp;</h3>\n                    <p class=\"mbr-text pb-3 mbr-fonts-style\" mbr-if=\"showText\" data-app-selector=\".mbr-text, .mbr-section-btn\" mbr-theme-style=\"display-4\">Monday – Friday\n                        <br>10.00 AM – 11.00 PM\n                        <br></p>\n                </div>\n\n            </div>\n\n\n            <div class=\"col-md-6 col-lg-3 pb-5 align-center\" mbr-if=\"menuItems1 > 3\">\n                <div class=\"item-content\">\n                    <h3 class=\"mbr-section-title pb-2 mbr-bold mbr-fonts-style\" mbr-if=\"showcardTitle\" mbr-theme-style=\"display-7\">Instagram feed\n                    </h3>\n                    <img class=\"feedimg\" src=\"../_images/img/01.jpg\" alt=\"Mobirise\">\n                    <img class=\"feedimg\" src=\"../_images/img/02.jpg\" alt=\"Mobirise\">\n                    <img class=\"feedimg\" src=\"../_images/img/03.jpg\" alt=\"Mobirise\">\n                </div>\n\n            </div>\n\n\n\n\n\n\n\n        </div>\n    </div>\n</section>",
